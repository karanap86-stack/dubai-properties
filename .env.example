# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
VITE_GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here

# AI Content Generation (OpenAI)
VITE_AI_API_KEY=sk_test_your_openai_api_key_here

# Backend API
VITE_BACKEND_URL=http://localhost:3000

# Email Service (SendGrid, Mailgun, etc.)
VITE_EMAIL_API_KEY=your_email_service_api_key_here

# Analytics
VITE_GA_ID=G-your_google_analytics_id

# App Mode
VITE_APP_MODE=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_GOOGLE_SHEETS=true
VITE_ENABLE_EMAIL=true
